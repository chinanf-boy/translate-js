{
  "name": "translate-mds",
  "version": "3.2.0",
  "description": "translate [folder/single] md file language to you want",
  "main": "./src/translateMds.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "demo": "node index.js md/",
    "ava": "ava --verbose --serial",
    "test": "nyc --reporter=lcov --reporter=text --reporter=html npm run ava",
    "pub": "npm run test && npm version patch && git push origin master && git push --tags",
    "changelog": "github_changelog_generator"
  },
  "husky": {
    "hooks": {}
  },
  "ava": {
    "files": [
      "test/**/*",
      "!test/*.Object.js",
      "!config.js",
      "!index.js"
    ]
  },
  "bin": {
    "translateMds": "./index.js"
  },
  "author": "yobrave",
  "license": "ISC",
  "dependencies": {
    "async": "^2.6.0",
    "debug": "^3.1.0",
    "meow": "^3.7.0",
    "mz": "^2.7.0",
    "ora-min": "^1.0.0",
    "remark": "^9.0.0",
    "translation.js": "^0.7.5",
    "turbocolor": "^2.6.1",
    "what-time": "^0.0.2",
    "winston": "^2.4.0"
  },
  "keywords": [
    "md",
    "translate"
  ],
  "devDependencies": {
    "ava": "^0.24.0",
    "husky": "1.0.0-rc.13",
    "nyc": "^11.3.0"
  }
}
