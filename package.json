{
  "name": "translate-mds",
  "version": "3.0.6",
  "description": "translate [folder] md file language to you want",
  "main": "./bin/translateExports.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "demo": "node index.js md/",
    "ava": "ava --verbose --serial",
    "test": "nyc --reporter=lcov --reporter=text --reporter=html npm run ava",
    "pubGitandNpm": "npm version patch && git push origin master && git push --tags",
    "changelog": "github_changelog_generator"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "ava": {
    "files": [
      "test/**/*",
      "!test/*.Object.js",
      "!config.js",
      "!index.js"
    ]
  },
  "bin": {
    "translateMds": "./index.js"
  },
  "author": "yobrave",
  "license": "ISC",
  "dependencies": {
    "async": "^2.6.0",
    "chalk": "^2.3.0",
    "meow": "^3.7.0",
    "mz": "^2.7.0",
    "ora": "^1.3.0",
    "remark": "^8.0.0",
    "translation.js": "^0.6.8",
    "winston": "^2.4.0"
  },
  "keywords": [
    "md",
    "translate"
  ],
  "devDependencies": {
    "ava": "^0.24.0",
    "husky": "^1.0.0-rc.2",
    "nyc": "^11.3.0"
  }
}
